{ 
    "manifest_version": 3,
    "name": "Chrome Blocker Extension",
    "version": "0.0.1",
    "description": "",
    "permissions": [
        "webNavigation",
        "tabs",
        "storage",
        "scripting",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ],
    "options_page": "options.html"
}